{
  "name": "store-adaptors",
  "version": "0.1.0",
  "description": "useSyncExternalStore adaptors and shim for using Svelte Readables in React.",
  "author": "Jillian Ada Burrows <jill@adaburrows.com>",
  "license": "CAL-1.0",
  "source": "./src/index.ts",
  "main": "./dist/store-adaptors.umd.js",
  "module": "./dist/store-adaptors.es.js",
  "exports": {
    ".": {
      "import": "./dist/store-adaptors.es.js",
      "require": "./dist/store-adaptors.umd.js"
    }
  },
  "types": "./dist/type_entry_point.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "clean": "rm -rf ./dist",
    "lint": "eslint . --ignore-path .gitignore --ext .ts",
    "check": "tsc --noEmit",
    "lint:fix": "pnpm run lint -- --fix",
    "test": "jest",
    "test:ci": "pnpm test",
    "test:watch": "pnpm test -- --watch --verbose"
  },
  "dependencies": {
    "react": "^16.14.0",
    "use-sync-external-store": "^1.2.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-modal": "^3.13.1",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^1.0.7",
    "eslint": "^8.22.0",
    "prettier": "^2.2.1",
    "react-refresh": "0.10.0",
    "react-refresh-typescript": "^2.0.4",
    "svelte": "^3.0.0",
    "terser": "4.4.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.0",
    "vite-plugin-dts": "^1.6.6",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "tsconfig-paths": "^3.13.0",
    "tslib": "^2.3.1"
  }
}
